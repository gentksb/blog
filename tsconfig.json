{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "paths": {
    "@components": ["src/components/*"],
    "@layouts": ["src/layouts/*"],
    "@lib": ["src/lib/*"],
    "@images": ["src/images/*"]
  },
  "exclude": ["./funcsions/**/*"]
}
